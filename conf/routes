# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           de.ironjan.pppb.controllers.HomeController.index
GET     /all_crawled                de.ironjan.pppb.controllers.HomeController.all_crawled
GET     /working_data_crawled       de.ironjan.pppb.controllers.HomeController.working_data_crawled
GET     /crawling_time_history      de.ironjan.pppb.controllers.HomeController.crawling_time_history

# An example controller showing how to write asynchronous code
GET     /message                    de.ironjan.pppb.controllers.AsyncController.message

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
